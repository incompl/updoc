<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>updoc</title>
    <link rel="stylesheet" href="../templates/default.css">
  </head>
  <body>
    <div id="content">
      <% _.each(sections, function(s) { %>
        <section class="depth<%= s.depth %> <%= s.header ? "header" : "" %>">
        
          <%= s.header ? "<h1>" + s.header + "</h1>" : "" %>
          
          <% if (s.name) { %>
            <h2>
              <%= s.type === "function" ? "function" : "" %>
              
              <%= s.type === "var" ? "var" : "" %>
              
              <%= s.name %>
            </h2>
          <% } %>
          
          <ul>
            <% _.each(s, function(val, prop) { %>
              <% if (prop !== "header" &&
                     prop !== "description" &&
                     prop !== "depth" &&
                     prop !== "module" &&
                     prop !== "type" &&
                     prop !== "name") { %>
                <li>
                  <span class="property">
                    <%= prop %>
                  </span>
                  <%= val %>
                </li>
              <% } %>
            <% }); %>
          </ul>
          
          <%= s.description ? '<div class="description">' + s.description + "</div>" : "" %>
          
        </section>
      <% }); %>
    </div>
    <footer>
      generated by <a href="https://github.com/incompl/updoc">updoc</a> <%= version %>
    </footer>
  </body>
</html>